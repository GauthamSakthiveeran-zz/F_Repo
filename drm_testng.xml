<suite name="Playback Web Regression Suite" verbose="1">
    <listeners>
        <listener class-name="com.ooyala.facile.listners.FacileTestListener" />
    </listeners>
    <test name="DRMRegression" parallel="classes" thread-count="1">
        <classes>
            <parameter name="testData" value="alice/DRM.xml"/>
            <parameter name="jsFile" value="alice/alice.js"/>
            <class name ="com.ooyala.playback.drm.PlaybackDRMTests"/>
            <class name="com.ooyala.playback.drm.PlaybackWithDRMLiveTests"/>
            <class name="com.ooyala.playback.drm.PlaybackWithoutDRMValidationsTests"/>
        </classes>
    </test>

</suite>

