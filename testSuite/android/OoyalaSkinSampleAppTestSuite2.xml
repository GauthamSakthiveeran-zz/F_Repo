<suite name="BasicPlayback SampleAPPTests Regression Suite"
	verbose="1">
	<listeners>
		<listener class-name="com.ooyala.facile.listners.FacileTestListener" />
	</listeners>

	<test name="BasicPlayback Regression" parallel="false"
		thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleAppDiscoveryNoAdsTest" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleAppSwipeDiscoveryScreenNoAdsTest" />
		</classes>
	</test>
	<test name="BasicPlayback " parallel="false" thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackApp" />
		</classes>
	</test>
