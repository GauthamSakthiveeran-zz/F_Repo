<elements area="sasport">
    <element key="SEARCH_BTN" findBy="id"
             id="searchAccountBtn" />
    <element key="CREATE_ENTITLEMENT_BTN"
             findBy="id" id="showEntitlementModalBtn" />
    <element key="CREATE_ENTITLEMENT_ID"
             findBy="id" id="createEntitlementContentId" />
    <element key="EXTERNAL_PRODUCT_ID"
             findBy="id" id="createEntitlementProdId" />
    <element key="MAX_DEVICES" findBy="id"
             id="createEntitlementNumDevices" />
    <element key="CREATE_BTN" findBy="id"
             id="createEntitlementBtn" />
    <element key="ACCOUNT_DEVICES_LIST" findBy="xPath"
             xPath="//div[@id='accountDevicesList']//div[normalize-space(@class)='row list-group-item container']" />
    <element key="DELETE_REGISTERED_DEVICE" findBy="xPath"
             xPath="//div[@id='accountDevicesList']/div[1]//button[text()='Delete']" />
    <element key="NO_DEVICES_REGISTERED" findBy="xPath"
             xPath=".//*[normalize-space(text())='No Devices registered']" />
    <element key="PCODE" findBy="id" id="pcode"></element>
    <element key="ACCOUNT_ID" findBy="id" id="accountId"></element>
    <element key="ENVIRONMENT" findBy="id" id="rlEnv"></element>
    <element key="ACCOUNT_DEVICES" findBy="id" id="accountDevicesContainer"></element>
</elements>