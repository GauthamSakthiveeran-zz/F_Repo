<suite name="OoyalaSkin SampleAPPTests Regression Suite" verbose="1">
     <listeners>
        <listener class-name="com.ooyala.facile.listners.FacileTestListener" />
    </listeners>
   	<test name="iOS OoyalaSkin BasicPlayer" parallel="false" thread-count="1">
		<classes>
			<parameter name="testData" value="ios/OoyalaSkinSampleAppBasicPlaybackTestData.xml" />
			<class name="com.ooyala.playback.apps.ios.ooyalaskinsampleapp.OoyalaSkinBasicPlaybackTest">
				<methods>
					<include name="testBasicPlayer" />
				</methods>
			</class>
		</classes>
	</test> 
 	<test name="iOS OoyalaSkin AdPlayer" parallel="false" thread-count="1">
		<classes>
			<parameter name="testData" value="ios/OoyalaSkinSampleAppBasicPlaybackAdTestData.xml" />
			<class name="com.ooyala.playback.apps.ios.ooyalaskinsampleapp.OoyalaSkinBasicPlaybackTest">
				<methods>
					<include name="testAdPlayer" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="iOS OoyalaSkin AdPlayer Addnl" parallel="false" thread-count="1">
		<classes>
			<parameter name="testData" value="ios/OoyalaSkinSampleAppBasicPlaybackAddnlAdTestData.xml" />
			<class name="com.ooyala.playback.apps.ios.ooyalaskinsampleapp.OoyalaSkinBasicPlaybackAddnlAdTest">
			</class>
		</classes>
	</test>
	
	<test name="iOS OoyalaSkin Discovery Test" parallel="false" thread-count="1">
		<classes>
			<parameter name="testData" value="ios/OoyalaSkinSampleAppBasicPlaybackTestData.xml" />
			<class name="com.ooyala.playback.apps.ios.ooyalaskinsampleapp.OoyalaSkinBasicPlaybackDiscoveryTest">
			</class>
		</classes>
	</test>
	
</suite>

