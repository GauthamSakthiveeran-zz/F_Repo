<elements area="watermark">
    <element key="CONTROL_BAR_ITEM" findBy="class"
             class="oo-control-bar-item" />
    <element key="WATERMARK_LOGO" findBy="xPath"
             xPath=".//*[@id='oo-responsive']//img" />
    <element key="FULLSCREEN_BTN" findBy="class"
             class="oo-icon-system-fullscreen" />
    <element key="NORMAL_SCREEN" findBy="class"
             class="oo-icon-system-minimizescreen" />
    <element key="OOYALA_LOGO" findBy="class"
             class="oo-logo" />
    <element key="STATE_SCREEN" findBy="class" class="oo-watermark-container" />

</elements>