<elements area="sharetab">
    <element key="SHARE_BTN" findBy="class"
             class="oo-icon-share" />
    <element key="MORE_OPTION_ITEM" findBy="class"
             class="oo-more-options-items" />
    <element key="SHARE_TAB" findBy="class"
             class="oo-share-tab" />
    <element key="CONTENT_SCREEN" findBy="class"
             class="oo-content-screen" />
    <element key="EMBED_TAB" findBy="class"  class="oo-embed-tab" />
    <element key="SHARE_CLOSE" findBy="class"
             class="oo-close-button" />
    <element key="SHARE_PANEL" findBy="class"
                class="oo-share-tab-panel" />
    <element key="SHARE_BUTTON" findBy="xPath"
             xPath=".//*[normalize-space(@class)='oo-icon oo-icon-share']" />
</elements>
