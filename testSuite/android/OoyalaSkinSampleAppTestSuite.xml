<suite name="BasicPlayback SampleAPPTests Regression Suite"
	verbose="1">
	<listeners>
		<listener class-name="com.ooyala.facile.listners.FacileTestListener" />
	</listeners>
 
	<test name="Ooyala Skin App Discovery No Ads Test" parallel="false"
		thread-count="1">
 		 <classes>
 			<parameter name="testData" value="OoyalaSkinSampleTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleAppDiscoveryNoAdsTest" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleAppSwipeDiscoveryScreenNoAdsTest" />
		</classes> 
		</test>
		<test name="Ooyala Skin App Basic Playback Test" parallel="false"
		thread-count="1">
		<classes> 
			<parameter name="testData" value="OoyalaSkinSampleTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackApp" />
		</classes>
		</test>
		<test name="Ooyala Skin App Vast2 Tests" parallel="false"
		thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleVASTTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackVASTAdsTest" />
		</classes> 
		</test>
 
 		<test name="Ooyala Skin App OoyalaAds Tests" parallel="false"
		thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleOoyalaAdTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackOoyalaAdsTest" />
		</classes> 
		</test>
		
		<test name="Ooyala Skin App Multi Ads Tests" parallel="false"
		thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleMultiAdTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackPreRollMidRollMultiAdsTest" />
		</classes> 
		</test>
		
		<test name="Ooyala Skin App Vast3 Ads Tests" parallel="false"
		thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinVast3AdPoddedTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackVAST3AdsTest" />
				<methods>
					<include name="testVast3Podded" />
				</methods>
		</classes>
	 	</test>
	 	
	 	<test name="Ooyala Skin App Vast3 Ads Tests Icons Test" parallel="false"
		thread-count="1">
		<classes>
 			<parameter name="testData" value="OoyalaSkinVast3AdWithIconTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackVAST3AdsTest" />
				<methods>
					<include name="testVast3AdWithIcon" />
				</methods>
		</classes>
		</test>
		
		<test name="Ooyala Skin App Vast3 Ads AllEvents Test" parallel="false"
		thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinVast3AdAllNewEventsTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackVAST3AdsTest" />
				<methods>
					<include name="testVast3AdAllNewEvents" />
				</methods>
		</classes>
		</test>

		<test name="Ooyala Skin App Share Test" parallel="false"
		thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackShareTest" />
		</classes>
		</test>
		
		<test name="Ooyala Skin App Vast3Ads Wrapper Test" parallel="false"
		thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinVast3AdWrapperTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackVAST3AdsTest" />
				<methods>
					<include name="testVast3AdWrapper" />
				</methods>
		</classes> 
	</test> 


	<test name="Freewheel Tests" parallel="false" thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleFW.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleAppFreewheelTests" />
		</classes>
	</test>

	 <test name="Google IMA Podded" parallel="false" thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleIMAPodded.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleAppIMATests">
				<methods>
					<include name="testPoddedAds" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="Google IMA Ad Rules" parallel="false" thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleIMAAdRules.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleAppIMATests">
				<methods>
					<include name="testAdRules" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="Google IMA Skippable Ads" parallel="false"
		thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleIMASkippableAds.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleAppIMATests">
				<methods>
					<include name="testSkippableAds" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="Google IMA Non-Ad rules" parallel="false"
		thread-count="1">
		<classes>
			<parameter name="testData" value="OoyalaSkinSampleIMANonAdRules.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleAppIMATests">
				<methods>
					<include name="testNonAdRules" />
				</methods>
			</class>
		</classes>
	</test> 
 	<test name="Ooyala Skin App VAMP Test" parallel="false"
		thread-count="1">
 		 <classes>
 			<parameter name="testData" value="OoyalaSkinSampleVAMPTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackVAMPTest" />
		</classes> 
		</test>
	
		 	<test name="Ooyala Skin App Vast3 Skippable Ads Tests" parallel="false"
		thread-count="1">
		<classes>
 			<parameter name="testData" value="OoyalaSkinVast3SkippableAdTests.xml" />
			<class
				name="com.ooyala.playback.apps.android.ooyalaskinsamplespp.OoyalaSkinSampleSkinPlayBackVAST3AdsTest" />
				<methods>
					<include name="testVast3AdSkippable" />
				</methods>
		</classes>
		</test>
	

</suite>

