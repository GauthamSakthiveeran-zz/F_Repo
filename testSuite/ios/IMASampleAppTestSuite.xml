<suite name="IMA SampleAPPTests Regression Suite" verbose="1">
	<listeners>
		<listener class-name="com.ooyala.facile.listners.FacileTestListener" />
	</listeners>
	<test name="IMA Podded Tests" parallel="false" thread-count="1">
		<classes>
			<parameter name="testData" value="ios/IMASampleAppsPoddedTests.xml" />
			<class
				name="com.ooyala.playback.apps.ios.imasampleapp.IMASampleAppsPoddedTests">
			</class>
		</classes>
	</test>
	<test name="IMA Tests" parallel="false" thread-count="1">
		<classes>
			<parameter name="testData" value="ios/IMASampleAppTests.xml" />
			<class
				name="com.ooyala.playback.apps.ios.imasampleapp.IMASampleAppTests">
			</class>
		</classes>
	</test>
</suite>

