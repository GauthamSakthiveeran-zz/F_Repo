<suite name="Playback Web Regression Suite" verbose="1">
    <listeners>
        <listener class-name="com.ooyala.facile.listners.FacileTestListener" />
    </listeners>

    <test name="VTC Web Regression Group" parallel="classes" thread-count="10">
        <classes>
            <parameter name="testData" value="VTC.xml"/>
            <parameter name="jsFile" value="vtc/vtc.js,common.js"/>
            <class name="com.ooyala.playback.VTC.PlaybackDiscoverTests" />
            <class name="com.ooyala.playback.VTC.PlaybackVolumeTests" />
            <class name="com.ooyala.playback.VTC.PlaybackVideoControllerEventTests" />
            <class name="com.ooyala.playback.VTC.PlaybackVerifyEventsTests" />
            <class name="com.ooyala.playback.VTC.PlaybackAdVideoSamePluginDiffElementTests" />
            <class name="com.ooyala.playback.VTC.PlaybackNewStreamTests" />
            <class name="com.ooyala.playback.playerfeatures.PlaybackClosedCaptionTests" />
            <class name="com.ooyala.playback.VTC.PlaybackSetEmbedCodeTests"/>
         </classes>
    </test>

    <test name = " VTC Multiple Player">
        <classes>
            <parameter name="testData" value="VTC.xml"/>
            <parameter name="jsFile" value="vtc/multiplePlayer.js"/>
            <class name="com.ooyala.playback.VTC.PlaybackMultipleV4PlayerTests" />
        </classes>
    </test>

     <test name="AMF Preroll VTC">
        <parameter name="jsFile" value="vtc/vtc.js,common.js,amf/amf.js"/>
        <parameter name="testData" value="VTC.xml"/>
        <classes>
            <class name="com.ooyala.playback.amf.preroll.PlaybackOverrideEncodingPriorityPrerollPoddedTests"></class>
            <class name="com.ooyala.playback.amf.preroll.PlaybackVerifyVideoElementCreatedAllPrerollAdsTests"></class>
        </classes>
    </test>

    <test name="AMF Midroll VTC" parallel="classes" thread-count="5">
		<parameter name="jsFile" value="vtc/vtc.js,common.js,amf/amf.js" />
		<parameter name="testData" value="VTC.xml"/>
		<classes>
			<class name="com.ooyala.playback.amf.midroll.PlaybackOverrideEncodingPriorityMidrollAdTests"></class>
			<class name="com.ooyala.playback.amf.midroll.PlaybackOverrideEncodingPriorityMidrollPoddedAdTests"></class>
			<class name="com.ooyala.playback.amf.midroll.PlaybackVerifyVideoElementCreatedAllMidrollAdsTests"></class>
		</classes>
	</test>

     <test name="AMF Postroll VTC" parallel="classes" thread-count="5">
        <parameter name="jsFile" value="common.js,vtc/vtc.js,common.js,amf/amf.js" />
        <parameter name="testData" value="VTC.xml"/>
        <classes>
            <class name="com.ooyala.playback.amf.postroll.PlaybackOverrideEncodingPriorityPostrollAdTests"></class>
            <class name="com.ooyala.playback.amf.postroll.PlaybackOverrideEncodingPriorityPostrollPoddedAdTests"></class>
            <class name="com.ooyala.playback.amf.postroll.PlaybackVerifyVideoElementCreatedAllPostrollAdsTests"></class>
        </classes>
    </test>

     <test name="AMF PreMidPost Roll VTC" parallel="classes" thread-count="5">
        <parameter name="jsFile" value="vtc/vtc.js,common.js,amf/amf.js" />
        <parameter name="testData" value="VTC.xml"/>
        <classes>
            <class name="com.ooyala.playback.amf.PlaybackOverrideEncodingPriorityPreMidPostAdTests"></class>
            <class name="com.ooyala.playback.amf.PlaybackOverrideEncodingPriorityPreMidPostPoddedAdTests"></class>
            <class name="com.ooyala.playback.amf.PlaybackVerifyVideoElementCreatedAllPreMiPostAdsTests"></class>
        </classes>
    </test>

</suite>